{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "declaration": true,
        "declarationMap": true,
        "emitDeclarationOnly": false,

        "outDir": ".dist",

        "skipLibCheck": true,

        "moduleResolution": "Node",

        "esModuleInterop": true
    },
    "include": ["src"],

    "exclude": [".dist"]
}
